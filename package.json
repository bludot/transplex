    "@nestjs/schedule": "^1.0.2",
    "@nestjs/swagger": "^5.0.9",
    "@nestjs/terminus": "^8.0.0",
    "@nestjs/typeorm": "^8.0.2",
    "@nestjs/websockets": "^8.2.4",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "bluebird": "^3.7.2",
    "cache-manager": "^3.6.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "handlebars": "^4.7.7",
    "lodash": "^4.17.21",
    "manticoresearch": "^2.0.2",
    "mjml": "^4.10.3",
    "node-mailjet": "^3.3.4",
    "nodemailer": "^6.6.5",
    "nyaapi": "^2.4.3",
    "parse-torrent": "^9.1.4",
